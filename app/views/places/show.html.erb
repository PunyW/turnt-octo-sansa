<% if notice %>
    <div class="panel">
      <h5><%= notice %></h5>
    </div>
<% end %>

<h3>
  <strong><%= @place.name %></strong>
</h3>
<p><%= @place.street %>, <%= "#{@place.zip} #{@place.city}" %></p>
<p><%= @place.country %></p>

<iframe
    width="600"
    height="450"
    frameborder="0" style="border:0"
    src="<%= @url %>">
</iframe>